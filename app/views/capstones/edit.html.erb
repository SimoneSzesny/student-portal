<h1>Edit Capstone!</h1>
<%= form_tag "/capstones/#{@capstone['id']}", method: :patch do %>
  <div>
    Name: <input type="text" name="name" value=<%= @capstone["name"] %>>
  </div>
  <div>
    Description: <input type="text" name="description" value=<%= @capstone["description"] %>>
  </div>
  <div>
    URL: <input type="text" name="url" value=<%= @capstone["url"] %>>
  </div>
  <div>
    Screen Shot: <input type="text" name="screenshot_url" value=<%= @capstone["screenshot_url"] %>>
  </div>
  <input type="submit" value="Submit">
<% end %> 